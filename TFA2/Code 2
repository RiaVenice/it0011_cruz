first = input("Enter Your First Name: ")
last = input("Enter Your Last Name: ")

print("Full Name: " + first + " " + last)
print("Full name (Upper Case): " + first.upper() + " " + last.upper())
print("Full name (Lower Case): " + first.lower() + " " + last.lower())
print("Lenght of Full Name: " + str(len(first + last)))

